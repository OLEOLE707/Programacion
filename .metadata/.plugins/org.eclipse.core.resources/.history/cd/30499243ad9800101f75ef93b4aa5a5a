package variables;

public class Tipo_de_variables {

	public static void main(String[] args) {
		
		byte edadAlumno; //Declaración
		edadAlumno=0; //Inicialización
		
		byte edadProfesor=0; //Declaración + Inicialización
		
		System.out.print("Enseñame la edad del alumno: "+edadAlumno);
	}

}
